{
  "name": "symfonylab/doctrine-orm-mutable-bundle",
  "type": "symfony-bundle",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "^4.3",
    "doctrine/doctrine-bundle": "^1.9",
    "doctrine/orm": "^2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "SymfonyLab\\DoctrineOrmMutable\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SymfonyLab\\DoctrineOrmMutable\\Tests\\": "Tests/"
    }
  }
}
